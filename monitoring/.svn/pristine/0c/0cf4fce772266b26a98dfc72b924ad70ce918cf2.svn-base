		<property name="sqlSessionFactory" ref="sqlSessionFactory"/>
	</bean>
	<bean id="cpuInfoImpl" class="com.ttmv.monitoring.collection.impl.CpuInfoImpl">
	   <property name="cpuInfoDaoMapper" ref="cpuInfoDaoMapper"></property>
	</bean>
