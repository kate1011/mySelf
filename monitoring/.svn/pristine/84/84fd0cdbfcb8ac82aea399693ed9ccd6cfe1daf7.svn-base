package com.ttmv.monitoring.entity;

import java.math.BigInteger;

public class LiveRoomInfo {
    
	private BigInteger id;							//ID
    private String rootId;							//房间编号
    private Integer unionFlag;					//是否连麦
    private Integer captureStatus;				//
    private Integer outputStatus;				//
    private Long anchorId;						//
    private Long partnerId;						//

    public BigInteger getId() {
        return id;
    }

    public void setId(BigInteger id) {
        this.id = id;
    }

    public String getRootId() {
        return rootId;
    }

    public void setRootId(String rootId) {
        this.rootId = rootId;
    }

    public Integer getUnionFlag() {
        return unionFlag;
    }

    public void setUnionFlag(Integer unionFlag) {
        this.unionFlag = unionFlag;
    }

    public Integer getCaptureStatus() {
        return captureStatus;
    }

    public void setCaptureStatus(Integer captureStatus) {
        this.captureStatus = captureStatus;
    }

    public Integer getOutputStatus() {
        return outputStatus;
    }

    public void setOutputStatus(Integer outputStatus) {
        this.outputStatus = outputStatus;
    }

    public Long getAnchorId() {
        return anchorId;
    }

    public void setAnchorId(Long anchorId) {
        this.anchorId = anchorId;
    }

    public Long getPartnerId() {
        return partnerId;
    }

    public void setPartnerId(Long partnerId) {
        this.partnerId = partnerId;
    }
}